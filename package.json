{
  "name": "update-files-action",
  "version": "0.1.0",
  "description": "A GitHub action for updating files.",
  "author": "Branislav Hol√Ω <branoholy@gmail.com>",
  "repository": "git@github.com/branoholy/branoholy-com.git",
  "bugs": {
    "url": "https://github.com/branoholy/update-files-action/issues"
  },
  "homepage": "https://github.com/branoholy/update-files-action#readme",
  "license": "MIT",
  "engines": {
    "node": "15.0.1"
  },
  "scripts": {
    "build": "webpack --env production",
    "build:dist": "npm run build && rm -rf dist/ && cp -r build/ dist/",
    "start": "node build/index.js",
    "start:dist": "node dist/index.js",
    "lint": "eslint 'src/**'",
    "lint:fix": "npm run lint -- --fix",
    "lint:all": "eslint '**' && prettier --check '**'",
    "lint:staged": "lint-staged",
    "type-check": "tsc",
    "type-check:watch": "tsc --watch",
    "node-versions-check": "babel-node --extensions '.ts' scripts/check-node-versions.ts",
    "node-versions-update": "babel-node --extensions '.ts' scripts/update-node-versions.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "stryker": "stryker run"
  },
  "dependencies": {
    "@actions/core": "1.2.6",
    "@octokit/rest": "18.0.6",
    "btoa": "1.2.1",
    "envalid": "6.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/node": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@stryker-mutator/core": "4.0.0",
    "@stryker-mutator/jest-runner": "4.0.0",
    "@types/btoa": "1.2.3",
    "@types/diff": "4.0.2",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.5",
    "@types/node-fetch": "2.5.7",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.6.0",
    "babel-jest": "26.6.1",
    "babel-loader": "8.1.0",
    "diff": "4.0.2",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-markdown": "1.0.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "husky": "4.3.0",
    "jest": "26.6.1",
    "lint-staged": "10.4.2",
    "node-fetch": "2.6.1",
    "prettier": "2.1.2",
    "typescript": "4.0.3",
    "webpack": "5.2.0",
    "webpack-cli": "4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "rm -f .lint-staged-temp-tsconfig.json",
      "prepare-commit-msg": "babel-node --extensions '.ts' hooks/prepare-commit-msg.ts $HUSKY_GIT_PARAMS"
    }
  }
}
