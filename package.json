{
  "name": "update-files-action",
  "version": "0.1.0",
  "description": "A GitHub action for updating files.",
  "author": "Branislav Hol√Ω <branoholy@gmail.com>",
  "repository": "git@github.com/branoholy/branoholy-com.git",
  "bugs": {
    "url": "https://github.com/branoholy/update-files-action/issues"
  },
  "homepage": "https://github.com/branoholy/update-files-action#readme",
  "license": "MIT",
  "engines": {
    "node": "15.3.0"
  },
  "scripts": {
    "build": "webpack --env production",
    "build:dist": "npm run build && rm -rf dist/ && cp -r build/ dist/",
    "start": "node build/index.js",
    "start:dist": "node dist/index.js",
    "lint": "eslint 'src/**'",
    "lint:fix": "npm run lint -- --fix",
    "lint:all": "eslint '**' && prettier --check '**'",
    "lint:staged": "lint-staged",
    "type-check": "tsc",
    "type-check:watch": "tsc --watch",
    "node-versions-check": "babel-node --extensions '.ts' scripts/check-node-versions.ts",
    "node-versions-update": "babel-node --extensions '.ts' scripts/update-node-versions.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "stryker": "stryker run",
    "postinstall": "git config --local core.hooksPath hooks"
  },
  "dependencies": {
    "@actions/core": "1.2.6",
    "@octokit/rest": "18.0.12",
    "btoa": "1.2.1",
    "envalid": "6.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.12.8",
    "@babel/core": "7.12.9",
    "@babel/node": "7.12.6",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-typescript": "7.12.7",
    "@stryker-mutator/core": "4.0.0",
    "@stryker-mutator/jest-runner": "4.1.1",
    "@types/btoa": "1.2.3",
    "@types/diff": "4.0.2",
    "@types/jest": "26.0.18",
    "@types/node": "14.14.11",
    "@types/node-fetch": "2.5.7",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "diff": "5.0.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-markdown": "1.0.2",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-simple-import-sort": "6.0.1",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "node-fetch": "2.6.1",
    "prettier": "2.2.1",
    "typescript": "4.1.2",
    "webpack": "5.10.0",
    "webpack-cli": "4.2.0"
  }
}
