{
  "name": "update-lock-file-action",
  "version": "0.1.0",
  "description": "A GitHub action for updating lock files.",
  "author": "Branislav Hol√Ω <branoholy@gmail.com>",
  "repository": "git@github.com/branoholy/branoholy-com.git",
  "bugs": {
    "url": "https://github.com/branoholy/update-lock-file-action/issues"
  },
  "homepage": "https://github.com/branoholy/update-lock-file-action#readme",
  "license": "MIT",
  "engines": {
    "node": "13.12.0"
  },
  "scripts": {
    "build": "webpack --env production",
    "build:dist": "npm run build && rm -rf dist/ && cp -r build/ dist/",
    "start": "node build/index.js",
    "start:dist": "node dist/index.js",
    "lint": "eslint 'src/**'",
    "lint:fix": "npm run lint -- --fix",
    "lint:all": "eslint '**' && prettier --check **/*.yaml",
    "lint:staged": "lint-staged",
    "type-check": "tsc",
    "type-check:watch": "tsc --watch",
    "dist-check": "npm run build && diff -s build/ dist/"
  },
  "dependencies": {
    "@actions/core": "1.2.3",
    "@octokit/rest": "17.3.0",
    "btoa": "1.2.1",
    "envalid": "6.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-typescript": "7.9.0",
    "@types/btoa": "1.2.3",
    "@types/node": "13.11.1",
    "@typescript-eslint/parser": "2.28.0",
    "babel-loader": "8.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-json": "2.1.1",
    "eslint-plugin-markdown": "1.0.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-simple-import-sort": "5.0.2",
    "husky": "4.2.5",
    "lint-staged": "10.1.4",
    "prettier": "2.0.4",
    "ts-node": "8.8.2",
    "typescript": "3.8.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "rm -f .lint-staged-temp-tsconfig.json",
      "prepare-commit-msg": "ts-node -O '{\"module\":\"commonjs\"}' hooks/prepare-commit-msg.ts $HUSKY_GIT_PARAMS"
    }
  }
}
