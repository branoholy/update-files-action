{
  "name": "update-files-action",
  "version": "0.1.0",
  "description": "A GitHub action for updating files.",
  "author": "Branislav Hol√Ω <branoholy@gmail.com>",
  "repository": "git@github.com/branoholy/branoholy-com.git",
  "bugs": {
    "url": "https://github.com/branoholy/update-files-action/issues"
  },
  "homepage": "https://github.com/branoholy/update-files-action#readme",
  "license": "MIT",
  "engines": {
    "node": "14.2.0"
  },
  "scripts": {
    "build": "webpack --env production",
    "build:dist": "npm run build && rm -rf dist/ && cp -r build/ dist/",
    "start": "node build/index.js",
    "start:dist": "node dist/index.js",
    "lint": "eslint 'src/**'",
    "lint:fix": "npm run lint -- --fix",
    "lint:all": "eslint '**' && prettier --check '**'",
    "lint:staged": "lint-staged",
    "type-check": "tsc",
    "type-check:watch": "tsc --watch",
    "node-versions-check": "babel-node --extensions '.ts' scripts/check-node-versions.ts",
    "node-versions-update": "babel-node --extensions '.ts' scripts/update-node-versions.ts"
  },
  "dependencies": {
    "@actions/core": "1.2.4",
    "@octokit/rest": "17.9.2",
    "btoa": "1.2.1",
    "envalid": "6.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/node": "7.8.7",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-typescript": "7.9.0",
    "@types/btoa": "1.2.3",
    "@types/diff": "4.0.2",
    "@types/node": "14.0.4",
    "@types/node-fetch": "2.5.7",
    "@typescript-eslint/parser": "3.0.0",
    "babel-loader": "8.1.0",
    "diff": "4.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-json": "2.1.1",
    "eslint-plugin-markdown": "1.0.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.4",
    "node-fetch": "2.6.0",
    "prettier": "2.0.5",
    "typescript": "3.9.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "rm -f .lint-staged-temp-tsconfig.json",
      "prepare-commit-msg": "babel-node --extensions '.ts' hooks/prepare-commit-msg.ts $HUSKY_GIT_PARAMS"
    }
  }
}
