{
  "name": "update-files-action",
  "version": "0.1.0",
  "description": "A GitHub action for updating files.",
  "author": "Branislav Hol√Ω <branoholy@gmail.com>",
  "repository": "git@github.com/branoholy/branoholy-com.git",
  "bugs": {
    "url": "https://github.com/branoholy/update-files-action/issues"
  },
  "homepage": "https://github.com/branoholy/update-files-action#readme",
  "license": "MIT",
  "engines": {
    "node": "17.2.0"
  },
  "scripts": {
    "build": "webpack --env production",
    "build:dist": "npm run build && rm -rf dist/ && cp -r build/ dist/",
    "start": "node build/index.js",
    "start:dist": "node dist/index.js",
    "lint": "eslint 'src/**' 'tests/**'",
    "lint:fix": "npm run lint -- --fix",
    "lint:all": "eslint '**' && prettier --check '**'",
    "lint:staged": "lint-staged",
    "lint:commit": "commitlint",
    "type-check": "tsc",
    "type-check:watch": "tsc --watch",
    "node-versions-check": "babel-node --extensions '.ts' scripts/check-node-versions.ts",
    "node-versions-update": "babel-node --extensions '.ts' scripts/update-node-versions.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest-e2e.config.js",
    "test:e2e:coverage": "npm run test:e2e -- --coverage",
    "test:e2e:watch": "npm run test:e2e -- --watch",
    "test:unit": "jest --config jest-unit.config.js",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:unit:watch": "npm run test:unit -- --watch",
    "stryker:e2e": "stryker run stryker-e2e.conf.js",
    "stryker:unit": "stryker run stryker-unit.conf.js",
    "postinstall": "[ -d .git ] && git config --local core.hooksPath hooks || echo 'Info: Git hooks not set outside a git repository.'"
  },
  "dependencies": {
    "@actions/core": "1.6.0",
    "@octokit/rest": "18.12.0",
    "btoa": "1.2.1",
    "envalid": "7.2.2"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/node": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-typescript": "7.16.0",
    "@commitlint/cli": "15.0.0",
    "@octokit/openapi-types": "11.2.0",
    "@stryker-mutator/core": "4.5.1",
    "@stryker-mutator/jest-runner": "4.5.1",
    "@stryker-mutator/typescript-checker": "4.5.1",
    "@types/btoa": "1.2.3",
    "@types/diff": "5.0.1",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.6",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-jest": "27.4.2",
    "babel-loader": "8.2.3",
    "conventional-changelog-conventionalcommits": "4.6.1",
    "diff": "5.0.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-markdown": "2.2.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "jest": "27.3.1",
    "lint-staged": "11.2.6",
    "nock": "13.2.1",
    "node-fetch": "3.1.0",
    "prettier": "2.4.1",
    "typescript": "4.4.4",
    "webpack": "5.62.1",
    "webpack-cli": "4.9.1"
  }
}
