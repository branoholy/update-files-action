{
  "name": "update-files-action",
  "version": "0.1.0",
  "description": "A GitHub action for updating files.",
  "author": "Branislav Hol√Ω <brano@holy.am>",
  "repository": "git@github.com/branoholy/branoholy-com.git",
  "bugs": {
    "url": "https://github.com/branoholy/update-files-action/issues"
  },
  "homepage": "https://github.com/branoholy/update-files-action#readme",
  "license": "MIT",
  "engines": {
    "node": "20.5.0"
  },
  "scripts": {
    "build": "webpack --env production",
    "build:dist": "npm run build && rm -rf dist/ && cp -r build/ dist/",
    "start": "node build/index.js",
    "start:dist": "node dist/index.js",
    "lint": "eslint 'src/**' 'tests/**'",
    "lint:fix": "npm run lint -- --fix",
    "lint:all": "eslint '**' && prettier --check '**'",
    "lint:staged": "lint-staged",
    "lint:commit": "commitlint",
    "type-check": "tsc",
    "type-check:watch": "tsc --watch",
    "node-versions-update": "babel-node --extensions '.ts' scripts/update-node-versions.ts",
    "test": "node --no-experimental-fetch node_modules/.bin/jest",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "test:e2e": "npm test -- --config jest-e2e.config.js",
    "test:e2e:coverage": "npm run test:e2e -- --coverage",
    "test:e2e:watch": "npm run test:e2e -- --watch",
    "test:unit": "npm test -- --config jest-unit.config.js",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:unit:watch": "npm run test:unit -- --watch",
    "stryker:e2e": "stryker run stryker-e2e.conf.js",
    "stryker:unit": "stryker run stryker-unit.conf.js",
    "postinstall": "[ -d .git ] && git config --local core.hooksPath hooks || echo 'Info: Git hooks not set outside a git repository.'"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@octokit/rest": "19.0.7",
    "btoa": "1.2.1",
    "envalid": "7.3.1",
    "glob": "10.3.3"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.4",
    "@babel/node": "7.22.6",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-typescript": "7.22.5",
    "@commitlint/cli": "17.6.7",
    "@octokit/openapi-types": "18.0.0",
    "@stryker-mutator/core": "6.1.2",
    "@stryker-mutator/jest-runner": "6.1.2",
    "@stryker-mutator/typescript-checker": "6.1.2",
    "@types/btoa": "1.2.3",
    "@types/diff": "5.0.3",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.3",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "babel-jest": "29.6.1",
    "babel-loader": "9.1.3",
    "conventional-changelog-conventionalcommits": "6.1.0",
    "diff": "5.1.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "jest": "29.6.1",
    "lint-staged": "13.2.3",
    "nock": "13.3.2",
    "node-fetch": "3.3.1",
    "prettier": "3.0.0",
    "typescript": "5.1.6",
    "webpack": "5.88.2",
    "webpack-cli": "5.0.1"
  }
}
